

  <!-- main begin -->
  <div class="container">

    <div class="portfolio">

      {{#each timeline.year}}
        <div class="year-row">

          <div class="time-nav">
            <h2 class="box year-box">
              {{#if current}}
                <span class="now">{{current}}</span>
              {{/if}}
              {{#if up}}
                <span class="year">{{up}}<i class="fa fa-arrow-up"></i></span>
              {{/if}}
              {{#if down}}
                <span class="year"><i class="fa fa-arrow-down"></i>{{down}}</span>
              {{/if}}
            </h2>
          </div>

          <div class="left-row">
              {{#each tiles.left}}
                {{> tile}}
              {{/each}}
          </div>

          <div class="right-row">
            {{#each tiles.right}}
              {{> tile}}
            {{/each}}
          </div>

        </div>
      {{/each}}

    </div>

  </div>
  <!-- main end -->
